<!-- Header -->
<header class="template-header">
	<div class="header-wrapper clearfix">
		<div class="header-logo">
        	{{widget('logo', 'logo', {"defaultlogo": asset('images/logo.png')})|raw}}
        </div>
        <div class="header-companyname">
        	{{widget('companyname', 'companyname', {"business": "profile"})|raw}}
        </div>
        <div class="header-extendednavigation">
        	{{widget('extendednavigation', 'extendednavigation', {"align":"right"})|raw}}
        </div>
        <div class="header-content">
        	{{widget('clicktocall', 'clicktocalldefault', {})|raw}}
        </div>
    </div>
</header>
<!-- End Header -->
